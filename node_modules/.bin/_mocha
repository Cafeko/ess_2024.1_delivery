C:/Users/Yano/Desktop/CIn_W/8op/SASE_REFRESH/ess_2024.1_delivery/#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,/,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/mocha/bin/mocha/bin/_mocha" "$@"
fi
